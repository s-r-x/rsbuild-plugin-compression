{
  "name": "rsbuild-plugin-compression",
  "description": "a compression plugin for rsbuild",
  "version": "0.1.1",
  "author": "Ilya Strus <strax1990@gmail.com>",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/s-r-x/rsbuild-plugin-compression"
  },
  "homepage": "https://github.com/s-r-x/rsbuild-plugin-compression",
  "bugs": {
    "url": "https://github.com/s-r-x/rsbuild-plugin-compression/issues"
  },
  "keywords": [
    "rsbuild",
    "rsbuild-plugin",
    "compress"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "lint:fix": "biome lint",
    "lint": "biome lint --write",
    "format": "biome format",
    "format:fix": "biome format --write",
    "check-types": "tsc --noEmit",
    "test": "node ./test/index.test.ts",
    "gen:docs": "typedoc ./src/index.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.14",
    "@rsbuild/core": "^1.7.3",
    "@rslib/core": "^0.19.6",
    "@types/node": "^24.10.13",
    "typedoc": "^0.28.17",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@rsbuild/core": "^1.0.0 || ^2.0.0-0"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "p-limit": "^7.3.0",
    "pretty-bytes": "^7.1.0"
  }
}
